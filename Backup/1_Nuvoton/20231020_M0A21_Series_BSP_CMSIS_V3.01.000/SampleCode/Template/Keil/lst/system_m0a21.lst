N
N#define DAC_CTL_DMAURIEN_Pos             (3)                                               /*!< DAC CTL: DMAURIEN Position             */
N#define DAC_CTL_DMAURIEN_Msk             (0x1ul << DAC_CTL_DMAURIEN_Pos)                   /*!< DAC CTL: DMAURIEN Mask                 */
N
N#define DAC_CTL_TRGEN_Pos                (4)                                               /*!< DAC CTL: TRGEN Position                */
N#define DAC_CTL_TRGEN_Msk                (0x1ul << DAC_CTL_TRGEN_Pos)                      /*!< DAC CTL: TRGEN Mask                    */
N
N#define DAC_CTL_TRGSEL_Pos               (5)                                               /*!< DAC CTL: TRGSEL Position               */
N#define DAC_CTL_TRGSEL_Msk               (0x7ul << DAC_CTL_TRGSEL_Pos)                     /*!< DAC CTL: TRGSEL Mask                   */
N
N#define DAC_CTL_OUTPUTEN_Pos             (8)                                               /*!< DAC CTL: OUTPUTEN Position             */
N#define DAC_CTL_OUTPUTEN_Msk             (0x1ul << DAC_CTL_OUTPUTEN_Pos)                   /*!< DAC CTL: OUTPUTEN Mask                 */
